<h2>Login</h2>
<form action="/auth/login" method="post">
    <%- include("_form_fields.ejs", {part: 1, email: false, formValues: locals.userFormData?.login}) %>
    <button type="submit">Login</button>
</form>
<h2>Register</h2>
<form action="/auth/register" method="post">
    <%- include("_form_fields.ejs", {part: 2, email: true, formValues: locals.userFormData?.register}) %>
    <button type="submit">Register</button>
</form>